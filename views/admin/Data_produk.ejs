    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1>Data Tables</h1>
            <a href="add_news" type="button" class="btn btn-primary">Tambah Brand</a>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="#">Home</a></li>
              <li class="breadcrumb-item active">Simple Tables</li>
            </ol>
          </div>
        </div>
      </div><!-- /.container-fluid -->
    </section>

    
        <!-- /.row -->
        <div class="row">
          <div class="col-12">
            <div class="card">
              <div class="card-header">
                <h3 class="card-title">Fixed Header Table</h3>

                <div class="card-tools">
                  <div class="input-group input-group-sm" style="width: 150px;">
                    <input type="text" name="table_search" class="form-control float-right" placeholder="Search">

                    <div class="input-group-append">
                      <button type="submit" class="btn btn-default"><i class="fas fa-search"></i></button>
                    </div>
                  </div>
                </div>
              </div>
              <!-- /.card-header -->
              <div class="card-body table-responsive p-0" style="height: 300px;">
                <table class="table table-head-fixed">
                  <thead>
                    <tr>
                      <th>#</th>
                      <th>Nama Produk</th>
                      <th>Deskirpsi</th>
                      <th>Gambar Produk</th>
                      <th>Harga Normal</th>
                      <th>Harga Diskon</th>
                      <th>Aksi</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% if(data.length){%>
                    <% for(i=0 ; i < data.length; i++){%>
                    <tr>
                      <td><%=data[i].id_news%></td>
                      <td><%=data[i].title%></td>
                      <td><%=data[i].description%></td>
                      <td><img src="/public/dist/img/<%=data[i].images%>" style="width: 160px; height: 160px;"></td>
                      <td><%=data[i].harga_normal%></td>
                      <td><%=data[i].harga_diskon%></td>
                      <td style="width: 20px; height: 20px;"><a href="/admin/Delete_news/<%= data[i].id_news %>" class="fas fa-trash-alt"><a href="/admin/edit_news/<%= data[i].id_news%>" class="fas fa-edit"></a></a></td>
                    </tr>
                    <%} } %> 
                  </tbody>
                </table>
                
              </div>
              <!-- /.card-body -->
            </div>
            <!-- /.card -->
          </div>
        </div>
        <!-- /.row -->
      